      <%= f.hidden_field :has_attended, value:false %>
      
      <div class="field">
      	<div class="field">
      			<%= f.label :name %>
      			<%= f.select :user_id, User.all.collect {|a| [ a.name + " " + a.email, a.id ]}, include_blank: true %>
      	</div>
      	
      	 <div class="field">
      	  <%= f.label :type %>
      	  <%= f.select :user_type, ['ADMIN', 'STUDENT'] %>
        </div>
      
      	<div class="field">
      		<%= f.hidden_field :_destroy %>
    			<%= link_to "remove", '#', onclick: 'remove_fields(this)' %>
      	</div>
    	</div>
    	<p></p>